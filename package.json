{
  "name": "demotest",
  "version": "0.0.1",
  "description": "demotest",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 18 || 19"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo .eslintcache",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "prepare": "npm run build",
    "rebuild": "npm run clean && npm run build",
    "migrate": "node ./dist/migrate",
    "start:dev": "nodemon ./src/index.ts"
  },
  "repository": {
    "type": "git"
  },
  "author": "Abdelrahman Ashraf <abdelrahman.asharaf@kortobaa.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.3",
    "@loopback/authentication-jwt": "^0.12.8",
    "@loopback/boot": "^5.0.8",
    "@loopback/core": "^4.0.8",
    "@loopback/repository": "^5.1.3",
    "@loopback/rest": "^12.0.8",
    "@loopback/rest-explorer": "^5.0.8",
    "bcryptjs": "^2.4.3",
    "isemail": "^3.2.0",
    "loopback-connector-postgresql": "^5.5.2",
    "multer": "^1.4.5-lts.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/testlab": "^5.0.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.36",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "npm start"
  }
}
